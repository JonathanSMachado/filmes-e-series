{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "yarn build:css && remix build",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "dev:css": "tailwindcss -i ./app/styles/tailwind_base.css -o ./app/styles/tailwind.css -w",
    "build:css": "tailwindcss -i ./app/styles/tailwind_base.css -o ./app/styles/tailwind.css"
  },
  "dependencies": {
    "@remix-run/react": "^1.2.2",
    "@remix-run/serve": "^1.2.2",
    "@tailwindcss/forms": "^0.4.0",
    "dotenv": "^16.0.0",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
