    "@types/nprogress": "^0.2.0",
    "@vercel/analytics": "^0.1.11",
    "domain-functions": "^1.4.0",
    "dotenv": "^16.0.0",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.39.7",
